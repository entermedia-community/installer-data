<page>
	<inner-layout>/${themeprefix}/layouts/asseteditor.html</inner-layout>
	<permission name="view">
		<not>
			<pagevalue name="cansendnotification" equals="false"/>
		</not>
	</permission>
	<path-action name="AssetEditModule.getAsset"  />
	<path-action name="Admin.loadPermissions"  />
	<path-action bean="commentArchive" name="PageValue.loadPageVariable"/>
	<property name="assetrootfolder">/${catalogid}/assets</property>
</page>